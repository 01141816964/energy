<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.omar.energy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.RoomDetailsActivity"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name=".ui.RegisterActivity" />
        <activity android:name=".ui.UsersListActivity" />
        <activity android:name=".ui.ChartActivity" />
        <activity android:name=".ui.SleepActivity" />
        <activity android:name=".ui.FirebaseActivity"></activity>
        <activity android:name=".ui.OutActivity" />
        <activity android:name=".ui.ExtraActivity" />
        <activity android:name=".ui.NormalActivity"></activity>

    </application>

</manifest>